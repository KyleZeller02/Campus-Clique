diff --git a/NightOut/Views/LoginView.swift b/NightOut/Views/LoginView.swift
new file mode 100644
index 0000000..104d91d
--- /dev/null
+++ b/NightOut/Views/LoginView.swift
@@ -0,0 +1,171 @@
+//
+//  ContentView.swift
+//  NightOut
+//
+//  Created by Kyle Zeller on 7/9/22.
+//
+
+import SwiftUI
+import Firebase
+
+extension Color {
+    static let Purple = Color(red: 15/255, green:4/255 , blue: 76/255)
+    static let White = Color(red: 238/255, green: 238/255, blue: 238/255)
+    static let Gray = Color(red: 120/255, green: 122/255, blue: 145/255, opacity:0.5)
+}
+struct LoginView: View {
+    @State var email: String = ""
+    @State var password: String = ""
+    @State var showView = false
+    
+    
+    var body: some View {
+        Color.White
+            .ignoresSafeArea()
+            .overlay(
+                NavigationView{
+                    VStack {
+                        Title()
+                        // this image(Logo) is a place holder for the app logo
+                        Logo()
+                        //username field
+                        TextField("Email", text: $email)
+                            .autocapitalization(.none)
+                            .padding()
+                            .background(Color.Gray)
+                            .cornerRadius(5.0)
+                            .padding(.bottom, 20)
+                        //password field
+                        SecureField("Password", text: $password)
+                            .autocapitalization(.none)
+                            .padding()
+                            .background(Color.Gray)
+                            .cornerRadius(5.0)
+                            .padding(.bottom, 20)
+                        //Login Buttons
+                        //normal login
+                        Button(action:
+                                {
+                            print("Button Tapped")
+                        }) {
+                            LoginButton()
+                        }
+                        //sign up button
+                        Button(action:
+                                { if email != "" && password != ""{
+                                    Auth.auth().createUser(withEmail: email, password: password){authResult, error in
+                                        if let e = error{
+                                            print(e)
+                                        }
+                                        //successfully regiesterd
+                                        else{
+                                            
+                                            //go to ProfileSetUP()
+                                            DispatchQueue.main.asyncAfter(deadline: .now() + 3) {
+                                                self.showView = true
+                                            }
+                                            
+                                        }
+                                    }
+                                    
+                                }}
+                        ) {
+                            SIGNUP()
+                        }
+                        HStack {
+                            //sign in with apple id
+                            Button(action:
+                                    {print("Button tapped")}) {
+                                LoginWithAppleID()
+                            }
+                            //sign in with phone number
+                            Button(action:
+                                    {print("Button tapped")}) {
+                                LoginWithPhoneNumber()
+                            }
+                        }
+                    }
+                    
+                }
+                    .navigationBarTitle("NightOut")
+                    .padding()
+                    .ignoresSafeArea()
+            )
+    }
+}
+
+struct ContentView_Previews: PreviewProvider {
+    static var previews: some View {
+        LoginView()
+            .previewInterfaceOrientation(.portrait)
+    }
+}
+
+struct Title: View {
+    var body: some View {
+        Text("NightOut")
+            .font(.system(size: 75))
+            .fontWeight(.semibold)
+            .padding(.bottom, 20)
+            .foregroundColor(Color.Purple)
+    }
+}
+
+struct Logo: View {
+    var body: some View {
+        Image("AppLogo")
+            .resizable()
+            .aspectRatio(contentMode: .fill)
+            .frame(width: 150, height: 150)
+            .clipped()
+            .cornerRadius(150)
+            .padding()
+    }
+}
+
+struct LoginButton: View {
+    var body: some View {
+        Text("LOGIN")
+            .font(.headline)
+            .foregroundColor(.white)
+            .padding()
+            .frame(width: 220, height: 60)
+            .background(.indigo)
+            .cornerRadius(15.0)
+    }
+}
+
+struct SIGNUP: View {
+    var body: some View {
+        Text("SIGN UP")
+            .font(.headline)
+            .foregroundColor(.white)
+            .padding()
+            .frame(width: 220, height: 60)
+            .background(.indigo)
+            .cornerRadius(15.0)
+    }
+}
+struct LoginWithAppleID: View {
+    var body: some View {
+        Text("Login With Apple ID")
+            .font(.system(size:12))
+            .foregroundColor(.white)
+            .padding()
+            .frame(width: 175, height: 60)
+            .background(.indigo)
+            .cornerRadius(15.0)
+    }
+}
+
+struct LoginWithPhoneNumber: View {
+    var body: some View {
+        Text("Login With Phone Number")
+            .font(.system(size:12))
+            .foregroundColor(.white)
+            .padding()
+            .frame(width: 175, height: 60)
+            .background(.indigo)
+            .cornerRadius(15.0)
+    }
+}
