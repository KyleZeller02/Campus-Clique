+struct UserProfileSetUp: View {
+    
+    @State var Major: String = ""
+    // Classes will need to be parsed to a string array
+    @State var Classes: String = ""
+    var body: some View {
+        
+        VStack{
+            Spacer()
+            Text("About You")
+                .font(.system(size: 40))
+                .fontWeight(.semibold)
+                .foregroundColor(Color.Purple)
+                .multilineTextAlignment(.center)
+                .padding(.top,200)
+            
+            
+            TextField("What Are You Studying?", text: $Major)
+                .autocapitalization(UITextAutocapitalizationType.words)
+                .padding()
+                .background(Color.Gray)
+                .cornerRadius(5.0)
+                .padding(.leading, 20)
+                .padding(.trailing, 20)
+            
+            
+            TextField("What Classes Are You Taking?", text: $Classes)
+                .autocapitalization(UITextAutocapitalizationType.words)
+                .padding()
+                .background(Color.Gray)
+                .cornerRadius(5.0)
+                .padding(.leading, 20)
+                .padding(.trailing, 20)
+            
+            
+            //I want the college to be a search and select like how crowdmark did
+            Spacer()
+            
+            Button(action:
+                    {
+                print("Next Button Pressed")
+            }) {
+                NextButton()
+            }
+            
+            
+        }
+    }
+}
